/* General styles */
QWidget {
    font-family: &quot;Calibra&quot;;
    color: #9d9995;
    background-color: #252528;/*#313136;*/ /*#4d4d4d;*/
    background-color: #323235;
    /*background: transparent;*/
}

/* set a common background color for all central widgets of dock widgets.
   All other QWidgets will have a transparent background. */
QDockWidget &gt; .QWidget, QDockWidget &gt; .QScrollArea, QDialog, QDockWidget#PythonInfoWidget &gt; .QWidget QScrollArea {
    background-color: #323235;
}

QDockWidget#PythonInfoWidget &gt; .QWidget {
    background-color: #252527;
}
QDockWidget#PythonInfoWidget &gt; .QWidget QScrollArea {
    /*border: none;*/
}


QStatusBar {
    background-color: #252527;
}
QMainWindow::separator {
    background: #444;
    width: 2px;
    height: 2px;
    border-style: outset;
    border-width: 1px;
}

QWidget#PropertyWidget {
}

/* Composite property */
QWidget#CompositeWidget {
    /*border: 1px solid #4a4a4f;*/
    border: 1px solid red;
    border: none;
    background: #2b2b2d;
    /*background: steelblue;*/
}

/* default background for nested composite properties with depth &gt; 4 */
QWidget#CompositeWidget &gt; .QWidget {
    background: lime;
}

QWidget[bgType=&quot;toplevel&quot;]#CompositeWidget {
    background-color: #323235; /* assign default background color */
}

QWidget[bgType=&quot;toplevel&quot;]#CompositeWidget &gt; .QWidget, 
QWidget[bgType=&quot;3&quot;]#CompositeWidget,
QWidget[bgType=&quot;0&quot;]#CompositeWidget &gt; .QWidget {
    background-color: #323235;
    background-color: #2a2a2e;
}
QWidget[bgType=&quot;0&quot;]#CompositeWidget, QWidget[bgType=&quot;1&quot;]#CompositeWidget &gt; .QWidget {
    background-color: #38383c;
    background-color: #242428;
    /*background-color: darkslategray;*/
}
QWidget[bgType=&quot;1&quot;]#CompositeWidget, QWidget[bgType=&quot;2&quot;]#CompositeWidget &gt; .QWidget {
    background: #323235;
    background-color: #2a2a2e;
    /*background-color: darkslateblue;*/
}
QWidget[bgType=&quot;2&quot;]#CompositeWidget, QWidget[bgType=&quot;3&quot;]#CompositeWidget &gt; .QWidget {
    background: #3a3a3d;
    background-color: #242428;
    /*background-color: maroon;*/
}


/*
QWidget#CompositeContents {
    border: 1px solid steelblue;
    background: #2b2b2d;
    background: red;
}
*/


QWidget#CompositeContents &gt; QWidget {
    border: 0px solid red;
    /*background: #2b2b2d;*/
}


/* Common defines for flat property editors, both active and disabled */
QLineEdit, QAbstractSpinBox, QComboBox {
    color: #c8ccd0;
    background: transparent;
    /* make background barely visible */
    /*background: #3a3a3d;*/
    /* change the border color to transparent here, if borders around properties 
      should be invisible. DO NOT adjust the border width as the widgets will then 
      jump on hover!
      */
    border: 1px solid #3a3a3e;
    border: 1px solid #3e3e3f;
    /*border: 1px solid transparent; */
}
QLineEdit:disabled, QAbstractSpinBox:disabled, QComboBox:disabled, QCheckBox:disabled {
    color: #777;
    /*border-color: #3c3c40;*/
    background: transparent;
}
QLineEdit:focus, QLineEdit:hover, QAbstractSpinBox:focus, QAbstractSpinBox:hover, QComboBox:on, QComboBox:focus, QComboBox:hover {
    background: #47474b;
    border: 1px solid #268bd2;
}



/* this renders all backgrounds unusable (labels, lineedit, etc.) due to specialization */
/*
QWidget#CompositeWidget QLabel, QWidget#CompositeWidget QWidget {
    border: none;
    background: transparent;
}
*/

QDockWidget#ConsoleWidget QTextEdit {
    border: none;
    border-bottom: 2px ridge #4a4a4f;
}

/* QToolBar styles */
QToolBar {
    spacing: 3px;
    padding: 1px;
    border: 1px solid black;
    background-color: #3a3a3e;
}
QToolBar::handle {
    background-image: url(:/stylesheets/images/dense6pattern.png);
    background-repeat: repeat-xy;
    background-color: #4a4a4f;
    border: 1px solid #333;
    width: 5px;
    margin:1px;
    margin-right: 3px;
    padding:1px;
   border-radius: 2px;
}

/* QToolButton */

QToolBar &gt; QToolButton {
    background-color: #3a3a3e;
    padding:2px;
}
QToolButton {
    border: 1px solid transparent;
    background-color: transparent;
}



QToolButton#resetButton {
    width: 13px;
    height: 13px;
    image: url(:/icons/reset-dark.png);
}
QToolButton:hover#resetButton {
    width: 13px;
    height: 13px;
    image: url(:/icons/reset.png);
}

/*QToolTop */
QToolTip {
    /*font-family: &quot;Courier&quot;;*/    
    color: #9d9995;
    background-color: #323235;  
    border-radius: 3px;
    border: 1px solid black;
    font-size: 14px;
    padding: 3px;   
}

/* QPushButton styles */
QPushButton {
    border: 1px solid black;
    border-radius: 4px;
    min-height: 20px;
    min-width: 80px;
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1,
                                stop:0 #6f6f75, stop:0.1 #323235, stop:0.9 #323235, stop:1 #222224);
} 
QWidget#CompositeWidget &gt; .QPushButton {
    /* overwrite minimum size for push buttons within the composite properties */
    min-height: 5px;
    min-width: 40px;    
}
QPushButton:default {
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1,
                                stop:0 #6f6f75, stop:0.1 #144c72, stop:0.9 #144c72, stop:1 #222224);
}
QPushButton:pressed {
    padding-top: 1px;
    padding-left: 1px;
    background: qlineargradient(x1:0, y1:1, x2:0, y2:0,
                                stop:0 #6f6f75, stop:0.15 #323235, stop:0.95 #323235, stop:1 #222224);
}
QPushButton:focus{
/*    border: 1px solid #268bd2;*/
}
QPushButton:disabled{
    color: #666;
    border: 1px solid #25272a;
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1,
                                stop:0 #6f6f75, stop:0.1 #3a3a3e, stop:0.9 #3a3a3e, stop:1 #222224);
}
QPushButton:flat {
     border: none; /* no border for a flat push button */
 }


/* QDockWidget styles */
QDockWidget::close-button, QDockWidget::float-button {
    padding: 0px;
    icon-size: 13px;
}
QDockWidget::title, inviwo--InviwoDockWidgetTitleBar {
    padding: 4px;
    padding-left: 5px;
    border-radius: 0px;
    border-top: 1px solid #888;
    border-left: 1px solid qlineargradient(x1:0, y1:0, x2:0, y2:1,
                                stop:0 #888, stop: 0.1 #888, stop:.5 #00888888);
    border-right: 1px solid qlineargradient(x1:0, y1:0, x2:0, y2:1,
                                stop:0 #888, stop: 0.1 #888, stop:.8 #00888888);
    border-bottom: 0px;
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1,
                                stop:0 #4a4a4e, stop:0.8 #323235, stop:1 #222224);
    /* smooth transition at the bottom */
/*
    padding-top: 6px;
    padding-bottom:-4px;
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1,
                                stop:0 #555, stop:0.7 #333, stop:.9 #222, stop: 1 #4D4D4D);
*/
}
QToolButton#dockBtn:hover {
    margin-left: 1px;
    margin-top: 1px;
    border: 1px solid #268bd2;
    border-radius: 3px;
}
QDockWidget {
    color: #999999;
    /*border: 0px;*/
    titlebar-close-icon: url(:/stylesheets/images/close.png);
    titlebar-normal-icon: url(:/stylesheets/images/undock.png);

    background-color: #222;
    border-width: 2px;
    /* border style does not work here... */
    /*
    border-color: white;
    border-style: dashed;
    */
}


QTabWidget {
}
QTabWidget::pane:bottom {
    border-bottom: 2px solid #414145;
    /*background: #414145; */ 
    /* cannot use other color than default background here because of QDockWidgets */
    /*background: #252527;*/
}
QTabWidget::pane:top {
    /*border-top: 2px solid #414145;*/
}
QTabWidget::pane QWidget, QTabWidget::pane QDockWidget QWidget {
    /* docked QDockWidgets ignore QTabWidget */
    background: transparent; /*#252527*/
}

QTabWidget::tab-bar {
    left: 5px;
}
QTabBar {
    /* docked QDockWidgets ignore QTabWidget */
    background: #252527;
}
QTabBar::tab {
    min-height: 20px;
    min-width: 80px;
    border: 1px solid #4a4a4f; /*#252527;*/
    border-radius: 3px;
    padding: 2px 10px 0px 10px;
    margin: 0px 1px 1px 2px; /* top, right (spacing between tabs), bottom, left */
}
QTabBar::tab:bottom {
    background-color: qlineargradient(x1:0, y1:0, x2:0, y2:1,
                               stop: 0 #1a1a1a, stop:0.3 #37373a, stop:0.9 #323235, stop:1 #222224);
    border-top-width: 0px;
    border-top-left-radius: 0px;
    border-top-right-radius: 0px;

    margin-top: 1px;
}
QTabBar::tab:top {
    background-color: qlineargradient(x1:0, y1:1, x2:0, y2:0,
                               stop: 0 #2a2a2a, stop:0.18 #323235, stop:0.88 #37373a, stop:1 #505058);
    border-bottom-width: 0px;
    border-bottom-left-radius: 0px;
    border-bottom-right-radius: 0px;

    margin-bottom: 1px;
}

QTabBar::tab:bottom:selected{
    background-color: qlineargradient(x1:0, y1:0, x2:0, y2:1,
                                stop:0 #323235, stop:0.9 #3c3c40, stop:1 #222224);
    border: 1px solid #888;
    color: #e9e4de;
    margin-top: -2px;
    /*font-weight: bold;*/
}
QTabBar::tab:top:selected{
    background-color: qlineargradient(x1:0, y1:1, x2:0, y2:0,
                                stop:0 #323235, stop:0.88 #3c3c40, stop:1 #5f5f5f);
    border: 1px solid #888;
    color: #e9e4de;
    margin-bottom: -2px;
    /*font-weight: bold;*/
}
QTabBar::tab:bottom:hover {
    border-left: 1px solid #268bd2;
    border-right: 1px solid #268bd2;
    border-bottom: 1px solid #268bd2;
    /*margin-top: -2px;*/ /* tab moves up a bit to indicate interaction */
}
QTabBar::tab:top:hover {
    border-left: 1px solid #268bd2;
    border-right: 1px solid #268bd2;
    border-top: 1px solid #268bd2;
    /*margin-top: -2px;*/ /* tab moves up a bit to indicate interaction */
}
QTabBar::tab:bottom:!selected {
}

/* QMenu styles */
QMenuBar,QMenuBar::item {
    color: #999999;
    background-color: #252527;
    selection-background-color: #3a3a3d;
}
QMenu,QMenu::item, QMenu::item::selected {
    color: #999999;
    selection-color: #999;
    background-color: #3a3a3d;
    selection-background-color: #47474b;
}
QMenu::item::disabled {
    color: #555;
}
QMenu {
    border: 1px solid #666;
}
QMenuBar::item::selected {
    color: white;
    background-color: #3a3a3d;   
    border-top: 1px solid #6a6a6f;
    border-left: 1px solid #6a6a6f;
    border-right: 1px solid #6a6a6f;
}
QMenu::separator {
    height: 0px;
    margin-top: 2px;
    margin-bottom: 2px;
    border-top: 2px groove #383838;
    margin-left: 25px;
}


/* QLabel styles */
QLabel {
    border: 0px;
}


/* QListView styles */
QListView::item:selected {
    border: 0px;
    background-color: #47474b;
}


/* QGroupBox styles */
QGroupBox {
    border: 1px solid #999999;
    border-radius: 5px;
    margin-top: 1ex;
}

QGroupBox::title {
    top: -7 ex;
    left: 10px;
    subcontrol-origin: border
}

/* QTreeView styles */
QTreeView {
    border: 1px solid #4a4a4f;
    show-decoration-selected: 1;
    selection-background-color: #65696d;
    outline: 0;
}

QTreeView::branch:hover{
    background-color: #47474b;
}
QTreeView::item:hover {
    background-color: #47474b;
}
QTreeView::item:selected, QTreeView::branch:selected {
    background-color: #65696d;
}
QTreeView::branch:has-children:!has-siblings:closed,
 QTreeView::branch:closed:has-children:has-siblings {
    border-image: none;
    image: url(:/stylesheets/images/arrow_lighter_right.png);
 }

 QTreeView::branch:open:has-children:!has-siblings,
 QTreeView::branch:open:has-children:has-siblings  {
    border-image: none;
    image: url(:/stylesheets/images/arrow_lighter_down.png);
 }

/* QRadioButton and QCheckBox styles */
QRadioButton::indicator, QCheckBox::indicator {
    width: 13px;
    height: 13px;
}
QRadioButton::indicator::unchecked {
    image: url(:/stylesheets/images/circle_gray.png);
}
QRadioButton::indicator::unchecked:disabled {
    image: url(:/stylesheets/images/circle_gray_disabled.png);
}
QRadioButton::indicator::checked {
    image: url(:/stylesheets/images/circle.png);
}
QRadioButton::indicator::checked:disabled {
    image: url(:/stylesheets/images/circle_disabled.png);
}
QCheckBox::indicator {
    border: 1px solid #1b1b1d;
    background: #47474b;
    background: #9d9995;
    border-radius: 1px;
}
QCheckBox::indicator:checked {
    background: #1e70a8;
}
QCheckBox::indicator:hover {
    border: 1px solid #268bd2;
}
QCheckBox::indicator:disabled {
    border-color: #3c3c40;
    background-color: #4f555b;
}
QCheckBox::indicator:checked:disabled {
    background-color: #65696d;
}

QCheckBox:indicator:hover {
    border: 1px solid #268bd2;
}



/* QScrollBar styles */
QScrollBar {
    border: 0px;
    padding: 2px;
}
QScrollBar:horizontal {
    height: 12px;
}
QScrollBar:vertical {
    width: 12px;
}
QScrollBar::handle {
    background: #1f1f21; /* dark */
    background: #625e5b; /* brownish / text */
    background: #5b5f62; /* blueish / background */
    border-width: 2px;
    border-radius: 4px;
    border: 0px;
}
QScrollBar::handle:hover {
    background: #1e70a8;
}
QScrollBar::handle:horizontal {
    min-width: 12px;
}
QScrollBar::handle:vertical {
    min-height: 26px;
}
QScrollBar::add-page, QScrollBar::add-line, QScrollBar::sub-page, QScrollBar::sub-line{
    background: none;
}

QTextBrowser {
    border-color: #444;
    border: 0px;
}

QPlainTextEdit {
    border: 1px solid #444;
}
QPlainTextEdit:hover  {
    background: #47474b;
    border: 1px solid #268bd2;
}

/* QLineEdit styles */
QLineEdit {
    border-radius: 1px;
}

QSplitter:handle {
    background: #c8ccd0;
    border: 1px solid #c8ccd0;
}
QSplitter::handle:pressed, QSplitter::handle:hover {
    background: #268bd2;
    border: 1px solid #268bd2;
}



/* QAbstractSpinBox styles */
QAbstractSpinBox {
    margin-left: 5px;
}
QAbstractSpinBox::up-button {
     image: url(:/stylesheets/images/arrow_lighter_up.png);
}
QAbstractSpinBox::down-button {
     image: url(:/stylesheets/images/arrow_lighter_down.png);
}
QAbstractSpinBox::up-button:disabled {
     image: url(:/stylesheets/images/arrow_up.png);
}
QAbstractSpinBox::down-button:disabled {
     image: url(:/stylesheets/images/arrow_down.png);
}

/* slim slider styles and part of the QSplitter styles */
QSlider::sub-page:horizontal, QSlider::add-page:vertical, 
QSplitter[Vertical=&quot;false&quot;]#RangeSliderQt QFrame#valueArea, 
QSplitter[Vertical=&quot;true&quot;]#RangeSliderQt QFrame#valueArea {
    border: 0px;
    background: #99a0a7;
    background: #1e70a8;
    border-radius:2px;
}
QSlider::sub-page:horizontal, QSplitter[Vertical=&quot;false&quot;]#RangeSliderQt QFrame#valueArea {
    margin: -1px 0px -1px 0px;
}
QSlider::add-page:vertical, QSplitter[Vertical=&quot;true&quot;]#RangeSliderQt QFrame#valueArea {
    margin: 0px -1px 0px -1px;
}
QSlider::groove, QSlider::add-page:horizontal, QSlider::sub-page:vertical, 
QSplitter[Vertical=&quot;false&quot;]#RangeSliderQt QFrame, QSplitter[Vertical=&quot;true&quot;]#RangeSliderQt QFrame {
    border: 0px;
    background: #65696d;
    border-radius:1px;
}
QSlider::groove:horizontal, QSlider::add-page:horizontal, QSplitter[Vertical=&quot;false&quot;]#RangeSliderQt QFrame {
    height: 3px;
}
QSlider::groove:vertical, QSlider::sub-page:vertical, QSplitter[Vertical=&quot;true&quot;]#RangeSliderQt QFrame {
    width: 3px;
}

QSlider::handle {
    background: #c8ccd0;
    border: 2px solid #c8ccd0;
    border-radius: 5px;
}
QSlider::handle:hover {
    background: #268bd2;
    border: 2px solid #268bd2;
}
QSlider::handle:horizontal {
    width: 8px;
    margin: -4px 0px; 
}
QSlider::handle:vertical {
    height: 8px;
    margin: 0px -4px; 
}


/* disabled slider / QSplitter */
QSlider::sub-page:horizontal:disabled, QSplitter[Vertical=&quot;false&quot;]#RangeSliderQt QFrame:disabled#valueArea, 
QSlider::add-page:vertical:disabled, QSplitter[Vertical=&quot;true&quot;]#RangeSliderQt QFrame:disabled#valueArea {
    background: #4f555b;
}

QSlider::groove:disabled, QSlider::add-page:horizontal:disabled, QSlider::sub-page:vertical:disabled, 
QSplitter:disabled[Vertical=&quot;false&quot;]#RangeSliderQt QFrame, QSplitter:disabled[Vertical=&quot;true&quot;]#RangeSliderQt QFrame {
    background: #404246;
}

QSlider::handle:disabled {
    background: #5d656b;
    border: 2px solid #5d656b;
}


/* QSplitter styles */
/*
QSplitter:vertical {
    height: 10px;
}
QSplitter:horizontal {
    width: 10px;
}
*/
/* main color definition for splitters is done with QSlider (see above) 
   only cosmetics are done here (rounded corners)!
*/



QSplitter[Vertical=&quot;false&quot;]#RangeSliderQt QFrame#valueArea, 
QSplitter[Vertical=&quot;true&quot;]#RangeSliderQt QFrame#valueArea {
    border: 0px;
    background: #99a0a7;
    background: #1e70a8;
    border-radius:0px;
}

QSplitter#RangeSliderQt QFrame, QSplitter#RangeSliderQt QFrame#valueArea {
    border-radius: 0px;    
    background-clip: content;
}
QSplitter[Vertical=&quot;true&quot;]#RangeSliderQt QFrame {
    margin: 0px 4px; 
    min-width: 4px;
    max-width: 4px;
}
QSplitter[Vertical=&quot;false&quot;]#RangeSliderQt QFrame {
    margin: 4px 0px; 
    min-height: 4px;
    max-height: 4px;
}
QSplitter[Vertical=&quot;true&quot;]#RangeSliderQt QFrame[LeftPart=&quot;false&quot;] {
    /* lower part of splitter */
    border-bottom-left-radius: 4px;
    border-bottom-right-radius: 4px;
}
QSplitter[Vertical=&quot;true&quot;]#RangeSliderQt QFrame[LeftPart=&quot;true&quot;] {
    /* upper part of splitter */
    border-top-left-radius: 4px;
    border-top-right-radius: 4px;
}
QSplitter[Vertical=&quot;false&quot;]#RangeSliderQt QFrame[LeftPart=&quot;true&quot;] {
    /* left part of splitter */
    border-top-left-radius: 4px;
    border-bottom-left-radius: 4px;
}
QSplitter[Vertical=&quot;false&quot;]#RangeSliderQt QFrame[LeftPart=&quot;false&quot;] {
    /* right part of splitter */
    border-top-right-radius: 4px;
    border-bottom-right-radius: 4px;
}
QSplitter[Vertical=&quot;true&quot;]#RangeSliderQt QFrame#valueArea {
    margin: 0px 3px; 
    min-width: 6px;
    max-width: 6px;
}
QSplitter[Vertical=&quot;false&quot;]#RangeSliderQt QFrame#valueArea {
    margin: 3px 0px; 
    min-height: 6px;
    max-height: 6px;
}

QSplitter::handle#RangeSliderQt {
    background: #c8ccd0;
    border: 2px solid #c8ccd0;
    border-radius: 6px;
}

QSplitter::handle:horizontal#RangeSliderQt {
    width: 5px;
    height: 5px;
}

QSplitter::handle:vertical#RangeSliderQt {
    height: 5px;
    width: 5px;
}


QSplitter::handle:pressed#RangeSliderQt, 
QSplitter::handle:hover#RangeSliderQt {
    background: #268bd2;
    border: 2px solid #268bd2;
}

QSplitter QFrame#pythonEditor, QFrame#htmlEditor, QTreeWidget#htmlTree {
    font-family: &quot;Calibra&quot;;
    background-color: #b0b0bc;
    color: #111111;
    border: 1px solid #9d9995;
